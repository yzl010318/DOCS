#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/admin/Desktop/DOCS/node_modules/.pnpm/qrcode@1.5.4/node_modules/qrcode/bin/node_modules:/mnt/c/Users/admin/Desktop/DOCS/node_modules/.pnpm/qrcode@1.5.4/node_modules/qrcode/node_modules:/mnt/c/Users/admin/Desktop/DOCS/node_modules/.pnpm/qrcode@1.5.4/node_modules:/mnt/c/Users/admin/Desktop/DOCS/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/admin/Desktop/DOCS/node_modules/.pnpm/qrcode@1.5.4/node_modules/qrcode/bin/node_modules:/mnt/c/Users/admin/Desktop/DOCS/node_modules/.pnpm/qrcode@1.5.4/node_modules/qrcode/node_modules:/mnt/c/Users/admin/Desktop/DOCS/node_modules/.pnpm/qrcode@1.5.4/node_modules:/mnt/c/Users/admin/Desktop/DOCS/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../qrcode@1.5.4/node_modules/qrcode/bin/qrcode" "$@"
else
  exec node  "$basedir/../../../../../qrcode@1.5.4/node_modules/qrcode/bin/qrcode" "$@"
fi
