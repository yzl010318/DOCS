#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/admin/Desktop/DOCS/node_modules/.pnpm/vuepress@2.0.0-rc.22_@vuepr_f0072aaabccea6dea31422810aa01019/node_modules/vuepress/bin/node_modules:/mnt/c/Users/admin/Desktop/DOCS/node_modules/.pnpm/vuepress@2.0.0-rc.22_@vuepr_f0072aaabccea6dea31422810aa01019/node_modules/vuepress/node_modules:/mnt/c/Users/admin/Desktop/DOCS/node_modules/.pnpm/vuepress@2.0.0-rc.22_@vuepr_f0072aaabccea6dea31422810aa01019/node_modules:/mnt/c/Users/admin/Desktop/DOCS/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/admin/Desktop/DOCS/node_modules/.pnpm/vuepress@2.0.0-rc.22_@vuepr_f0072aaabccea6dea31422810aa01019/node_modules/vuepress/bin/node_modules:/mnt/c/Users/admin/Desktop/DOCS/node_modules/.pnpm/vuepress@2.0.0-rc.22_@vuepr_f0072aaabccea6dea31422810aa01019/node_modules/vuepress/node_modules:/mnt/c/Users/admin/Desktop/DOCS/node_modules/.pnpm/vuepress@2.0.0-rc.22_@vuepr_f0072aaabccea6dea31422810aa01019/node_modules:/mnt/c/Users/admin/Desktop/DOCS/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../vuepress/bin/vuepress.js" "$@"
else
  exec node  "$basedir/../vuepress/bin/vuepress.js" "$@"
fi
